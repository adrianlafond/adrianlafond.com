{
  "name": "adrianlafond.com",
  "version": "1.0.0",
  "description": "adrian's website",
  "main": "index.html",
  "scripts": {
    "test": "jest --coverage",
    "start": "npm run del:dev && concurrently \"npm run webpack:watch\" \"npm run jekyll:serve\" \"npm run serve:live\"",
    "build": "npm run del:prod && webpack --config webpack/config.prod",
    "del:prod": "del ./js/*",
    "del:dev": "del ./_site/js/*",
    "webpack:dev": "webpack --config webpack/config.dev",
    "webpack:watch": "npm run del:dev && npm run webpack:dev && webpack --config webpack/config.dev --watch",
    "serve:live": "live-server _site --ignore \"node_modules,_site/node_modules\" --watch \"_site\" --no-browser",
    "jekyll:serve": "jekyll serve",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianlafond/adrianlafond.com.git"
  },
  "author": "Adrian Lafond",
  "license": "",
  "bugs": {
    "url": "https://github.com/adrianlafond/adrianlafond.com/issues"
  },
  "homepage": "https://github.com/adrianlafond/adrianlafond.com#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "del-cli": "^1.1.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "live-server": "^1.2.0",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {}
}
